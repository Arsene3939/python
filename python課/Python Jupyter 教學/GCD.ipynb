   "source": [
    "def gcd(a,b,s0=0,s1=1):\n",
    "    if a%b==0:\n",
    "        return (b,s1)\n",
    "    return gcd(b,a%b,s1,s0+(a//b)*s1)\n",
    "gcd(24,60)"
   ]
