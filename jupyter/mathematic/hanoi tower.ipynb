    "def ht(n,a='A',b='B',c='C'):\n",
    "    if n==1:\n",
    "        print(\"move plate%d from pole %s to pole %s\"%(n,a,c))\n",
    "        return\n",
    "    ht(n-1,a,c,b)\n",
    "    print(\"move plate%d from pole %s to pole %s\"%(n,a,c))\n",
    "    ht(n-1,b,a,c)\n",
    "ht(5)"
   ]
