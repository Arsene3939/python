    "def binary(lens,num):\n",
    "    x=bin(num)\n",
    "    x=list(x)\n",
    "    for i in range(lens):\n",
    "        if i<lens-len(x)+2:\n",
    "            print('0',end='')\n",
    "        else:\n",
    "            print(x[i-lens+len(x)],end='')\n",
    "    print()\n",
    "binary(15,160*128)\n"
   ]
  }
